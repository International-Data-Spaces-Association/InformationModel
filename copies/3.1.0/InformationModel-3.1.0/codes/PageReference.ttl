@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix idsc: <https://w3id.org/idsa/code/> .
@prefix idsm: <https://w3id.org/idsa/metamodel/> .

# Instances
# ---------

# Content pagination reflects the notion of a visual web page pagination.
# Below are the names of absolute and relative references to a "content" page.

idsc:FIRST
    a ids:PageReferenceParameter ;
    rdfs:label "first"@en ;
    rdfs:comment "First page of a content view"@en .

idsc:LAST
    a ids:PageReferenceParameter ;
    rdfs:label "last"@en ;
    rdfs:comment "Last page of a content view"@en .

idsc:PREVIOUS
    a ids:PageReferenceParameter ;
    rdfs:label "previous"@en ;
    rdfs:comment "Previous page of a content view"@en .

idsc:NEXT
    a ids:PageReferenceParameter ;
    rdfs:label "next"@en ;
    rdfs:comment "Next page of a content view"@en .

