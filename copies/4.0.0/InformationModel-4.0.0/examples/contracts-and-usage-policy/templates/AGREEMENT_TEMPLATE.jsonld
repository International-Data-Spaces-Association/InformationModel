// idsc:AgreementTemplate
// ------------------

// The AgreementTemplate is a RegEx-like representation of the possible forms, which can be inserted at the Agreement position in an IDS Usage Control Template.

{
    "@context": "https://w3id.org/idsa/contexts/context.jsonld",
    "@type": "ids:ContractAgreement",
    "@id": "?URI",
    "ids:provider": (idsc:PARTICIPANT_TEMPLATE),
    "ids:consumer": (idsc:PARTICIPANT_TEMPLATE)
    (, "ids:permission": (idsc:AGREEMENT_PERMISSION_TEMPLATE) )+    // at least one Permission
}
