ids:years 
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in years"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "years duration"@en ;
	rdfs:range xsd:decimal .
ids:months
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in months"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "months duration"@en ;
	rdfs:range xsd:decimal .
ids:weeks
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in weeks"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "weeks duration"@en ;
	rdfs:range xsd:decimal .
ids:days
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in days"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "days duration"@en ;
	rdfs:range xsd:decimal .
ids:hours
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in hours"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "hours duration"@en ;
	rdfs:range xsd:decimal .
ids:minutes
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in minutes"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "minutes duration"@en ;
	rdfs:range xsd:decimal .
ids:seconds
	a owl:DatatypeProperty;
	rdfs:comment "length of, or element of the length of, a temporal extent expressed in seconds"@en ;
	rdfs:domain ids:GeneralDurationDescription ;
	rdfs:label "seconds duration"@en ;
	rdfs:range xsd:decimal .

ids:numericDuration a owl:DatatypeProperty;
	rdfs:subPropertyOf time:numericDuration;
	rdfs:domain ids:Duration;
	rdfs:range xsd:decimal;
	rdfs:label "numeric duration"@en;
	rdfs:comment "Value of a temporal extent expressed as a number scaled by a temporal unit"@en.
	
ids:unitType a owl:ObjectProperty;
	rdfs:subPropertyOf time:unitType;
	rdfs:domain ids:Duration;
	rdfs:range ids:TemporalUnit;
	rdfs:label "unit type"@en;
	rdfs:comment "The temporal unit which provides the precision of a date-time value or scale of a temporal extent"@en.
	

ids:date
    a owl:DatatypeProperty;
    rdfs:subPropertyOf time:inXSDDate;
    rdfs:domain ids:Instant;
    rdfs:range xsd:date;
    rdfs:label "date"@en;
    rdfs:comment "Date coordinate of the instant."@en;
    idsm:example "2018-12-31";
    rdfs:seeAlso <https://www.w3.org/TR/xmlschema11-2/#date> ;
    .

ids:dateTime
    a owl:DatatypeProperty;
    rdfs:subPropertyOf time:inXSDDateTimeStamp;#  time-zone is mandatory
    rdfs:domain ids:Instant;
    rdfs:range xsd:dateTime;
    rdfs:label "date time"@en;
    rdfs:comment "Date time with a defined time zone."@en;
    idsm:example "2018-12-31T13:20:00+02:00";
    rdfs:seeAlso <https://www.w3.org/TR/xmlschema11-2/#dateTimeStamp> ;
    .

ids:begin
    a owl:ObjectProperty;
    rdfs:subPropertyOf time:hasBeginning;
    rdfs:domain ids:Interval;
    rdfs:range ids:Instant;
    rdfs:label "beginning"@en;
    rdfs:comment "Beginning of an Interval."@en.

ids:end
    a owl:ObjectProperty;
    rdfs:subPropertyOf time:hasEnd;
    rdfs:domain ids:Interval;
    rdfs:range ids:Instant;
    rdfs:label "end"@en;
    rdfs:comment "End of an Interval."@en.



