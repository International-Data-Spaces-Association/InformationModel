@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .

# Classes
# -------

ids:IndustrialClassification a owl:Class;
    idsm:abstract true;
    rdfs:label "Industrial classification"@en ;
    rdfs:comment "The class of industries that can be distinguished by their activities.".

ids:IndustrialClassificationISIC rdfs:subClassOf ids:IndustrialClassification;
    a owl:Class;
    rdfs:label "ISIC Industrial Classification"@en ;
    rdfs:comment "Classification according to International Standard Industrial Classification (ISIC).".


