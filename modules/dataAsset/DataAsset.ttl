@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

@prefix ids_da: <https://schema.industrialdataspace.org/dataAsset/> .
@prefix ids_base: <https://schema.industrialdataspace.org/base/> .
@prefix ids_temp: <https://schema.industrialdataspace.org/temporalEntity/> .
@prefix ids_spatial: <https://schema.industrialdataspace.org/spatialEntity/> .
@prefix ids_rel: <https://schema.iais.fraunhofer.de/ids/helper/relation/> .
@prefix ids_doc: <https://schema.iais.fraunhofer.de/ids/helper/documentation/> .
@prefix ids_trans: <https://schema.iais.fraunhofer.de/ids/helper/transformation/> .

# Classes
# -------

ids_da:DataAsset rdfs:subClassOf ids_base:EntityWithLifecycle;
    rdfs:subClassOf dcat:Dataset;
    a owl:Class;
    rdfs:label "DataAsset"@en;
    rdfs:comment "DataAsset on the Industrial Dataspace. May be a data offering, data that is in the state of being transfered or an aggregation of multiple other dataassets."@en;
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversTemporal;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversSpatial;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversCategory;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversLanguage;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:keyword;
        ids_rel:relationType ids_rel:OneToMany;
    ].

# Properties
# ----------

ids_da:version rdfs:subPropertyOf dct:hasVersion;
    a owl:DatatypeProperty;
    rdfs:label "version"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:string;
    rdfs:comment "Version identifier of the InfrastructureComponent."@en.

ids_da:licenseDocument rdfs:subPropertyOf dct:license;
    a owl:ObjectProperty;
    rdfs:label "licenseDocument"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_da:LicenseDocument;
    ids_trans:refByURI true;
    rdfs:comment "License of the DataAsset. Refers to common standard licenses used in the IDS."@en.

ids_da:licenseReference rdfs:subPropertyOf dct:license;
    a owl:ObjectProperty;
    rdfs:label "licenseReference"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:anyURI;
    rdfs:comment "References a license document that is publicly available on the Web."@en.

ids_da:coversTemporal rdfs:subPropertyOf dct:temporal;
    rdfs:label "coversTemporal"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_temp:TemporalEntity;
    rdfs:comment "Temporal period or instance covered by the DataAsset."@en.

ids_da:coversSpatial rdfs:subPropertyOf dct:spatial;
    rdfs:label "coversSpatial"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_spatial:SpatialEntity;
    rdfs:comment "Spatial location covered by the DataAsset."@en.

ids_da:coversCategory rdfs:subPropertyOf dcat:theme;
    rdfs:label "coversCategory"@en;
    ids_doc:labelPluralForm "coversCategories"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:anyURI;
    rdfs:comment "Links to a taxonomy categorizing content-related coverage of the DataAsset."@en.

ids_da:keyword rdfs:subPropertyOf dcat:theme;
    rdfs:label "keyword"@en;
    ids_doc:labelPluralForm "keywords"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:anyURI;
    rdfs:comment "Links to an item in a flat list of controlled keywords that describe content-related coverage of the DataAsset."@en.

ids_da:coversLanguage rdfs:subPropertyOf dct:language;
    rdfs:label "coversLanguage"@en;
    ids_doc:labelPluralForm "coversLanguages"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_da:Language;
    ids_trans:refByURI true;
    rdfs:comment "States the language that is used in the DataAsset."@en.

ids_da:origin a owl:DatatypeProperty;
    rdfs:label "origin"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:anyURI;
    rdfs:comment "Links to the original creator or entity from which the DataAsset has been derived."@en.